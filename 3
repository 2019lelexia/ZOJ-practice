#include<stdio.h>

int fa=0,fb=0;

void judge(int a,int b,int cnt){ //wingback

    if(b==1){
        fb=1;
        if(a==1){
            fa=1;
        }
    }
    if(cnt==1||(a==1&&b==1)){
        return;
    }
    if(a%cnt==0){
        judge(a/cnt,b,cnt-1);
    }
    if(b%cnt==0){
        judge(a,b/cnt,cnt-1);
    }
    judge(a,b,cnt-1);
}

int main(){
    int a,b,t;
    while(scanf("%d %d",&a,&b)!=EOF){
        fa=0;
        fb=0;
        if(a<b){
            t=a;
            a=b;
            b=t;
        }
        judge(a,b,100);
        if(fa==0&&fb==1){
            printf("%d\n",b);
        }
        else{
            printf("%d\n",a);
        }
    }
}
